{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2022"],
    "types": ["node"],
    "noEmit": true
  },
  "include": [
  "src/server.ts",
  "src/core/**/*.ts",
  "src/services/api/**/*.ts",
  "src/services/entrance/**/*.ts",
  "src/services/database/**/*.ts"
  ],
  "exclude": [
  "src/**/*.tsx",
  "src/build-ins/**",
  "src/components/**",
  "src/uikit/**",
  "src/services/api/trpc/react.tsx",
  "src/services/api/trpc/utils.ts"
  ]
}
